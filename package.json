{
  "name": "scraper-master",
  "version": "1.1.0",
  "description": "Master module for scraper",
  "main": "src/index.ts",
  "scripts": {
    "dev": "concurrently \"yarn watch-compile\" \"yarn watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=production node dist/index.js",
    "prisma-seed": "yarn build && yarn prisma db seed",
    "clean": "rm -rf dist"
  },
  "prisma": {
    "seed": "node dist/seed/seed.js"
  },
  "author": "Shree Krishna Lamichhane",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.21",
    "@types/node": "^18.11.9",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@prisma/client": "4.7.1",
    "dotenv": "^16.0.3",
    "prisma": "^4.7.1",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4"
  }
}
